    <scriptTask id="insurance-created-reply" name="Skicka 8z &quot;Försäkring Skapad&quot;" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>importPackage(Packages.rikardholm.insurance.application.messaging.message);

println('OCR: ' + ocr);
println('Insurance: ' + insuranceNumber);

var message = new InsuranceCreatedResponse(ocr, personalIdentifier, insuranceNumber);

outboxRepository.save(message);</script>
