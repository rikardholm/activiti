  <process id="insurance-transfer" name="Insurance Transfer" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <callActivity id="callactivity1" name="Skapa försäkring" calledElement="register-insurance">
      <extensionElements>
        <activiti:in source="personalIdentifier" target="personalIdentifier"></activiti:in>
        <activiti:in source="address" target="address"></activiti:in>
        <activiti:out source="insuranceNumber" target="insuranceNumber"></activiti:out>
      </extensionElements>
    </callActivity>
    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>importPackage(Packages.rikardholm.insurance.domain.customer);

var personalIdentifier=PersonalIdentifier.of('871201-3045');
var address = Address.of('Testgatan 8');

execution.setVariable('personalIdentifier', personalIdentifier);
execution.setVariable('address',address);
</script>
    </scriptTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="scripttask1" targetRef="callactivity1"></sequenceFlow>
    <scriptTask id="scripttask2" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>importPackage(Packages.rikardholm.insurance.domain.insurance);

var insuranceNumber = execution.getVariable('insuranceNumber');

println('Insurance: ' + insuranceNumber);</script>
    </scriptTask>
    <sequenceFlow id="flow3" sourceRef="callactivity1" targetRef="scripttask2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow4" sourceRef="scripttask2" targetRef="endevent1"></sequenceFlow>
  </process>
