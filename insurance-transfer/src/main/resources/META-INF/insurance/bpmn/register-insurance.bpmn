<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="register-insurance" name="Registrera en försäkring" isExecutable="true">
    <startEvent id="start" name="Start">
      <extensionElements>
        <activiti:formProperty id="personalIdentifier" name="Personnummer" variable="form.personalIdentifier"></activiti:formProperty>
        <activiti:formProperty id="address" name="address" variable="form.address"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <endEvent id="end" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="scripttask1"></sequenceFlow>
    <scriptTask id="scripttask1" name="Konvertera formulär" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>importPackage(Packages.rikardholm.insurance.domain.customer);

var personalIdentifierString = execution.getVariable('form.personalIdentifier');
var personalIdentifier = PersonalIdentifier.of(personalIdentifierString);

if (execution.hasVariable('form.address')) {
  var addressString = execution.getVariable('form.address');
  var address = Address.of(addressString);
  execution.setVariable('address', address);
}

execution.setVariable('personalIdentifier', personalIdentifier);</script>
    </scriptTask>
    <sequenceFlow id="flow3" sourceRef="scripttask1" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Sök efter kund" activiti:expression="${customerRepository.findBy(personalIdentifier).isPresent()}" activiti:resultVariableName="customerExists"></serviceTask>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="scripttask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${customerExists}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="scripttask2" name="Registrera försäkring på kund" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var customer = customerRepository.findBy(personalIdentifier).get();
var insuranceNumber = insuranceRegistration.register(customer).getInsuranceNumber();
execution.setVariable('insuranceNumber', insuranceNumber);</script>
    </scriptTask>
    <sequenceFlow id="flow5" sourceRef="scripttask2" targetRef="end"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="scripttask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!customerExists && execution.hasVariable('address')}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="servicetask2" targetRef="scripttask2"></sequenceFlow>
    <serviceTask id="servicetask2" name="Skapa kund" activiti:expression="${customerRegistration.register(personalIdentifier, address)}"></serviceTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow8" sourceRef="servicetask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway1" targetRef="servicetask3">
      <documentation>Kunden finns inte, och addressen har inte angetts</documentation>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!customerExists && !execution.hasVariable('address')}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="exclusivegateway2" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${lookupSuccessful && foundPerson && !secretAddress}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask3" name="Slå upp adress i SPAR" activiti:expression="${sparDelegate.lookup(execution, personalIdentifier)}"></serviceTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow11" sourceRef="servicetask3" targetRef="exclusivegateway2"></sequenceFlow>
    <endEvent id="errorendevent1" name="ErrorEnd">
      <errorEventDefinition></errorEventDefinition>
    </endEvent>
    <sequenceFlow id="flow12" sourceRef="exclusivegateway2" targetRef="errorendevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${lookupSuccessful && !foundPerson}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask1" name="MO fixar biffen" activiti:candidateGroups="Middle Office">
      <extensionElements>
        <activiti:formProperty id="address" name="Adress" variable="form.address"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow13" sourceRef="exclusivegateway2" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!lookupSuccessful || (foundPerson && secretAddress)}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="usertask1" targetRef="scripttask3"></sequenceFlow>
    <scriptTask id="scripttask3" name="Konvertera adress" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>importPackage(Packages.rikardholm.insurance.domain.customer);

var addressString = execution.getVariable('form.address');
var address = Address.of(addressString);
execution.setVariable('address', address);
println('Address: ' + addressString);</script>
    </scriptTask>
    <sequenceFlow id="flow15" sourceRef="scripttask3" targetRef="servicetask2"></sequenceFlow>
  </process>
