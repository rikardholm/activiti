    <scriptTask activiti:autoStoreVariables="false" activiti:exclusive="true" id="scripttask1" name="Skapa beställning från meddelande" scriptFormat="javascript">
      <script><![CDATA[print('Skapar beställning…');
var PersonalIdentifier = Java.type('rikardholm.insurance.domain.customer.PersonalIdentifier');
var personnummer = execution.getVariable('messagePayload').personalIdentifier;
execution.setVariable('personnummer',PersonalIdentifier.of(personnummer));]]></script>
    </scriptTask>
    <scriptTask activiti:autoStoreVariables="false" activiti:exclusive="true" id="scripttask2" name="Sök efter försäkringar på personnummer" scriptFormat="javascript">
      <script><![CDATA[var personnummer = execution.getVariable('personnummer');
print('Letar efter '  + personnummer);
var insurances = insuranceFinder.findByPersonnummer(personnummer);
print('Antal försäkringar funna: ' + insurances.size());
execution.setVariable('insuranceCount',insurances.size());]]></script>
    </scriptTask>
    <serviceTask activiti:exclusive="true" activiti:expression="${messageSender.sendInsuranceInformationResponse(execution)}" id="servicetask1" name="Skicka information om försäkringar"/>
    <serviceTask activiti:exclusive="true" activiti:expression="${messageSender.sendNoInsurancesResponse(personnummer)}" id="servicetask2" name="Meddela att det inte finns några försäkringar"/>
  </process>
